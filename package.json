{
  "name": "ass",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "ASTRO_TELEMETRY_DISABLED=1 astro dev",
    "build": "ASTRO_TELEMETRY_DISABLED=1 astro build",
    "preview": "ASTRO_TELEMETRY_DISABLED=1 astro preview",
    "astro": "ASTRO_TELEMETRY_DISABLED=1 astro",
    "test": "bun run test:unit && bun run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:watch": "PWTEST_WATCH=1 playwright test",
    "format:ts": "prettier --write",
    "format:js": "prettier --write",
    "format:astro": "prettier --write --plugin prettier-plugin-astro",
    "format:json": "prettier --write",
    "format:md": "prettier --write",
    "format:yaml": "prettier --write",
    "format:toml": "taplo fmt",
    "format:css": "prettier --write",
    "format:html": "prettier --write",
    "format:bash": "shfmt -w",
    "format:fish": "fish_indent --write",
    "format:dockerfile": "dockerfmt --write",
    "lint:ts": "eslint",
    "lint:js": "eslint",
    "lint:astro": "eslint",
    "lint:json": "eslint",
    "lint:md": "markdownlint-cli2",
    "lint:yaml": "yq eval",
    "lint:toml": "taplo check",
    "lint:css": "stylelint",
    "lint:html": "html-validate",
    "lint:bash": "shellcheck",
    "lint:caddyfile": "caddy validate --config"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.16",
    "astro": "5.15.3",
    "tailwindcss": "4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@playwright/test": "1.56.1",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "@vitest/browser": "4.0.6",
    "@vitest/browser-playwright": "4.0.6",
    "eslint": "9.39.1",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "globals": "16.5.0",
    "html-validate": "10.3.0",
    "markdownlint-cli2": "0.18.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "stylelint": "16.25.0",
    "typescript-eslint": "8.46.3",
    "vitest": "4.0.6"
  }
}
